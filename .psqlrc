\x auto
\set ON_ERROR_STOP on
\set ON_ERROR_ROLLBACK interactive
\set QUIET 1
\encoding UTF8
\set AUTOCOMMIT off
--\pset null '¤'
--\pset null '⁜'
\pset null '[NULL]'
--\pset linestyle 'unicode'
--\pset unicode_border_linestyle single
--\pset unicode_column_linestyle single
--\pset unicode_header_linestyle double
set intervalstyle to 'postgres_verbose';
\setenv LESS '-iMFXSx4R'
--\setenv EDITOR '/Applications/Emacs.app/Contents/MacOS/bin/emacsclient -nw'
--\setenv EDITOR '~/.local/bin/rmate -w'
\setenv EDITOR '/opt/homebrew/bin/nvr -s'
--\setenv EDITOR 'open -a bbedit -w --resume'
--\set PROMPT1 '%[%033[1;33;40m%]%n%[%033[0m%]@%[%033[34m%]%m%[%033[0m%]/%[%033[32m%/%R%[%033[0m%]%# '
\set PROMPT1 '%[%033[1;33m%]%n%[%033[0m%]@%[%033[34m%]%m%[%033[0m%]/%[%033[32m%/%R%[%033[0m%]%x%# '
\set PROMPT2 ''
\set COMP_KEYWORD_CASE lower
\pset pager always
\timing
\set VERBOSITY verbose
\set HISTFILE ~/.psql_history- :DBNAME
\set HISTCONTROL ignoredups
\set HISTSIZE 2000
\unset QUIET
\set IGNOREEOF 5
\pset format wrapped
set application_name to frank_psql; commit;
set bytea_output to escape; commit;
